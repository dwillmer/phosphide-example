{
  "name": "phosphide-example",
  "version": "0.0.2",
  "description": "An example Phosphide plugin application.",
  "main": "index.js",
  "dependencies": {
    "codemirror": "^5.9.0",
    "phosphide": "^0.2.0",
    "phosphor-command": "^0.6.0",
    "phosphor-di": "^0.9.0",
    "phosphor-messaging": "^1.0.6",
    "phosphor-widget": "^1.0.0-rc.0"
  },
  "devDependencies": {
    "css-loader": "^0.23.1",
    "http-server": "^0.8.5",
    "rimraf": "^2.4.4",
    "style-loader": "^0.13.0",
    "typescript": "^1.6.2",
    "webpack": "^1.12.9"
  },
  "scripts": {
    "clean": "rimraf blue/*.js && rimraf green/*.js && rimraf red/*.js && rimraf yellow/*.js && rimraf editor/*.js",
